{
  "name": "explicit-object-mapper-ng",
  "version": "4.0.3",
  "description": "allows mapping of named fields between javascript objects with optional transforms",
  "main": "lib/index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "pretest": "yarn lint",
    "test": "jest",
    "lint": "eslint --ignore-path .gitignore .",
    "format": "prettier --write '**/*.{ts,js,yaml}'",
    "build": "tsc"
  },
  "dependencies": {
    "lodash.isfunction": "^3.0.9",
    "lodash.isstring": "^4.0.1"
  },
  "devDependencies": {
    "@types/assert": "^1.4.3",
    "@types/jest": "^24.0.24",
    "@types/lodash.isfunction": "^3.0.6",
    "@types/lodash.isstring": "^4.0.6",
    "@types/node": "^12.12.21",
    "@types/pretty-hrtime": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "pretty-hrtime": "^1.0.3",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/opentable/explicitobjectmap-node.git"
  },
  "keywords": [
    "mapper",
    "mapper",
    "transform"
  ],
  "author": "steve higgs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stevejhiggs/explicitobjectmap-node-ng"
  },
  "homepage": "https://github.com/stevejhiggs/explicitobjectmap-node-ng",
  "files": ["lib/**/*"],
  "jest": {
    "rootDir": "./",
    "preset": "ts-jest",
    "testMatch": [
      "<rootDir>/tests/**/*.{spec,test}.{ts,tsx}"
    ]
  }
}
