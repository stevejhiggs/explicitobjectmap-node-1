{
  "name": "explicit-object-mapper-ng",
  "version": "4.0.4",
  "description": "allows mapping of named fields between javascript objects with optional transforms",
  "main": "lib/index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "pretest": "yarn lint",
    "test": "jest",
    "lint": "eslint --ignore-path .gitignore .",
    "format": "prettier --write '**/*.{ts,js,yaml}'",
    "build": "tsc"
  },
  "dependencies": {
    "lodash.isfunction": "^3.0.9",
    "lodash.isstring": "^4.0.1"
  },
  "devDependencies": {
    "@types/assert": "^1.5.1",
    "@types/jest": "^26.0.4",
    "@types/lodash.isfunction": "^3.0.6",
    "@types/lodash.isstring": "^4.0.6",
    "@types/node": "^14.0.23",
    "@types/pretty-hrtime": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "pretty-hrtime": "^1.0.3",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stevejhiggs/explicitobjectmap-node-ng.git"
  },
  "keywords": [
    "mapper",
    "mapper",
    "transform"
  ],
  "author": "steve higgs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stevejhiggs/explicitobjectmap-node-ng"
  },
  "homepage": "https://github.com/stevejhiggs/explicitobjectmap-node-ng",
  "files": ["lib/**/*"],
  "jest": {
    "rootDir": "./",
    "preset": "ts-jest",
    "testMatch": [
      "<rootDir>/tests/**/*.{spec,test}.{ts,tsx}"
    ]
  }
}
